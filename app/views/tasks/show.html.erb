<%= render "home/dashboard" %>
<div class="container">
  <p>
    <strong>Hora Ejecución:</strong>
    <%= @task.execution_time %>
  </p>
  
  <p>
    <strong>Nombre Tarea:</strong>
    <%= @task.name %>
  </p>
  
  <p>
    <strong>Descripción:</strong>
    <%= @task.description %>
  </p>
  
  <p>
    <strong>Task Id:</strong>
    <%= @task.task_id %>
  </p>
  
  <%= link_to 'Editar', edit_task_path(@task), class:"btn btn-warning"%> |
  <%= link_to '<< Volver', tasks_path, class:"btn btn-info" %>

</div>
