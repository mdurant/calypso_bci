<p id="notice"><%= notice %></p>

<h1>Listing Profile Teams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Celular</th>
      <th>Anexo</th>
      <th>Address work</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_teams.each do |profile_team| %>
      <tr>
        <td><%= profile_team.name %></td>
        <td><%= profile_team.email %></td>
        <td><%= profile_team.phone %></td>
        <td><%= profile_team.celular %></td>
        <td><%= profile_team.anexo %></td>
        <td><%= profile_team.address_work %></td>
        <td><%= profile_team.image %></td>
        <td><%= link_to 'Show', profile_team %></td>
        <td><%= link_to 'Edit', edit_profile_team_path(profile_team) %></td>
        <td><%= link_to 'Destroy', profile_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile team', new_profile_team_path %>
