<%= render "home/dashboard"%>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div class="well fondo-form">
        
        <h2>Se han ingresado los siguientes datos.</h2>
        <p>
          <strong>Titulo:</strong>
          <small><%= @hito.title %></small>
        </p>
        <p>
          <strong>Negocio:</strong>
          <%= @hito.area.title %>
        </p>
      
        <p>
          <strong>Date from:</strong>
          <%= @hito.date_from %>
        </p>
      
        <p>
          <strong>Date end:</strong>
          <%= @hito.date_end %>
        </p>
      
        <p>
          <strong>Benefit:</strong>
          <%= @hito.benefit %>
        </p>
      <%= link_to '<< Listado', hitos_path, class: "btn btn-info" %>
      </div>
    </div>
  </div>
</div>